./App.css\";\nimport Navbar from \"./Components/Navbar/Navbar\";\nimport Showcase from \"./Components/Showcase/Showcase\";\nimport Needies from \"./Components/Needies/Needies\";\nimport Footer from \"./Components/Footer/Footer\";\nimport { BrowserRouter as Router, Route, Switch } from \"react-router-dom\";\nimport FirmFund from \"./Components/Firm/FirmFund\";\nimport Success from \"./Components/Firm/Success\";\nimport { AuthProvider } from \"./AuthContext\";\nimport Login from \"./Components/Authentication/Login\";\nimport Signup from \"./Components/Authentication/Signup\";\n\nfunction App() {\n  return (\n    <div className=\"App\">\n      <Router>\n        <AuthProvider>\n          <Navbar />\n          <Switch>\n            <Route exact path=\"/\" component={Showcase} />\n            <Route path=\"/login\" component={Login} />\n            <Route path=\"/signup\" component={Signup} />\n            <Route path=\"/needies\" component={Needies} />\n            <Route path=\"/:firm_id\" component={FirmFund} />\n          </Switch>\n          <Footer />\n        </AuthProvider>\n      </Router>\n    </div>\n  );\n}\n\nexport default App;\n","/home/shivansh/Desktop/codejam-21/sassy-sandwiches/src/firebase.js",[],"/home/shivansh/Desktop/codejam-21/sassy-sandwiches/src/Components/Firm/Success.js",[],"/home/shivansh/Desktop/codejam-21/sassy-sandwiches/src/Components/Firm/FirmFund.js",["61"],"import React, { useEffect, useState } from \"react\";\nimport \"./FirmFund.css\";\nimport { makeStyles } from \"@material-ui/core/styles\";\nimport Modal from \"@material-ui/core/Modal\";\nimport Backdrop from \"@material-ui/core/Backdrop\";\nimport Fade from \"@material-ui/core/Fade\";\nimport { useHistory } from \"react-router-dom\";\nimport { db } from \"../../firebase\";\n\nconst useStyles = makeStyles((theme) => ({\n  modal: {\n    display: \"flex\",\n    alignItems: \"center\",\n    justifyContent: \"center\",\n  },\n  paper: {\n    backgroundColor: theme.palette.background.paper,\n    border: \"2px solid #000\",\n    boxShadow: theme.shadows[5],\n    padding: theme.spacing(2, 4, 3),\n  },\n}));\n\nfunction FirmFund() {\n  const classes = useStyles();\n  let url = window.location.pathname;\n  let arr = url.split(\"/\");\n  const id = arr[1];\n\n  const [open, setOpen] = useState(false);\n  const [firm_image, setFirm_image] = useState(\"\");\n  const [firm_name, setFirm_name] = useState(\"\");\n  const [about_firm, setAbout_firm] = useState(\"\");\n  const [need_funds, setNeed_funds] = useState(\"\");\n  const [amount, setAmount] = useState(0);\n  const [filled_amount, setFilled_amount] = useState(\"\");\n\n  useEffect(() => {\n    db.collection(\"users\")\n      .doc(id)\n      .get()\n      .then((doc) => {\n        if (doc.exists) {\n          setFirm_image(doc.data().firm_image);\n          setAbout_firm(doc.data().about_firm);\n          setNeed_funds(doc.data().need_funds);\n          setFirm_name(doc.data().firm_name);\n          setAmount(doc.data().amount);\n        } else {\n          console.log(\"No such firm!\");\n        }\n      })\n      .catch((err) => {\n        console.log(err);\n      });\n  });\n\n  const handleSubmit = (e) => {\n    e.preventDefault();\n  };\n\n  const handleOpen = (e) => {\n    e.preventDefault();\n    setOpen(true);\n  };\n\n  const handleClose = () => {\n    setOpen(false);\n    db.collection(\"users\")\n      .doc(id)\n      .update({ amount: amount - filled_amount });\n    setFilled_amount(\"\");\n  };\n\n  return (\n    <div className=\"firmFund\">\n      <Modal\n        className={classes.modal}\n        open={open}\n        onClose={handleClose}\n        closeAfterTransition\n        BackdropComponent={Backdrop}\n        BackdropProps={{\n          timeout: 500,\n        }}\n      >\n        <Fade in={open}>\n          <div className={classes.paper}>\n            <h2>Thanks for Donation!</h2>\n            <p>\n              You have helped {firm_name} by ${filled_amount}\n            </p>\n          </div>\n        </Fade>\n      </Modal>\n\n      <div className=\"firmFund_image\">\n        <img src={firm_image} alt=\"\" />\n      </div>\n      <div className=\"firmFund_about\">\n        <h2>About our Firm</h2>\n        <p>{about_firm}</p>\n      </div>\n      <div className=\"firmFund_need\">\n        <h2>Why do we need Funds?</h2>\n        <p>{need_funds}</p>\n      </div>\n      <div className=\"firmFund_fund\">\n        <h2>\n          Fund Req: ${amount - filled_amount < 0 ? 0 : amount - filled_amount}\n        </h2>\n        <form onSubmit={handleSubmit}>\n          <input\n            type=\"number\"\n            required\n            placeholder=\"Enter the Amount...\"\n            min={0}\n            max={amount}\n            value={filled_amount}\n            onChange={(e) => setFilled_amount(e.target.value)}\n          />\n          <button\n            onClick={handleOpen}\n            disabled={!filled_amount || filled_amount > amount}\n          >\n            Fund\n          </button>\n        </form>\n      </div>\n    </div>\n  );\n}\n\nexport default FirmFund;\n","/home/shivansh/Desktop/codejam-21/sassy-sandwiches/src/Components/Needies/Needies.js",[],"/home/shivansh/Desktop/codejam-21/sassy-sandwiches/src/Components/Showcase/Showcase.js",["62"],"/home/shivansh/Desktop/codejam-21/sassy-sandwiches/src/Components/Footer/Footer.js",[],"/home/shivansh/Desktop/codejam-21/sassy-sandwiches/src/Components/Needies/Needy.js",[],"/home/shivansh/Desktop/codejam-21/sassy-sandwiches/src/Components/Navbar/Navbar.js",[],"/home/shivansh/Desktop/codejam-21/sassy-sandwiches/src/AuthContext.js",["63"],"import React, { createContext, useState, useEffect, useContext } from \"react\";\nimport { auth } from \"./firebase\";\n\nconst AuthContext = createContext();\n\nexport function useAuth() {\n  return useContext(AuthContext);\n}\n\nexport function AuthProvider({ children }) {\n  const [currentUser, setCurrentUser] = useState();\n\n  function signup(email, password) {\n    return auth.createUserWithEmailAndPassword(email, password);\n  }\n\n  function login(email, password) {\n    return auth.signInWithEmailAndPassword(email, password);\n  }\n\n  function logout() {\n    return auth.signOut();\n  }\n\n  useEffect(() => {\n    const unsubscribe = auth.onAuthStateChanged((user) => {\n      setCurrentUser(user);\n    });\n  }, []);\n\n  const value = {\n    currentUser,\n    signup,\n    login,\n    logout,\n  };\n\n  return <AuthContext.Provider value={value}>{children}</AuthContext.Provider>;\n}\n","/home/shivansh/Desktop/codejam-21/sassy-sandwiches/src/Components/Authentication/Signup.js",["64"],"/home/shivansh/Desktop/codejam-21/sassy-sandwiches/src/Components/Authentication/Login.js",[],{"ruleId":"65","replacedBy":"66"},{"ruleId":"67","replacedBy":"68"},{"ruleId":"69","severity":1,"message":"70","line":9,"column":8,"nodeType":"71","messageId":"72","endLine":9,"endColumn":15},{"ruleId":"69","severity":1,"message":"73","line":7,"column":10,"nodeType":"71","messageId":"72","endLine":7,"endColumn":20},{"ruleId":"69","severity":1,"message":"74","line":16,"column":9,"nodeType":"71","messageId":"72","endLine":16,"endColumn":20},{"ruleId":"69","severity":1,"message":"75","line":26,"column":11,"nodeType":"71","messageId":"72","endLine":26,"endColumn":22},{"ruleId":"69","severity":1,"message":"76","line":1,"column":17,"nodeType":"71","messageId":"72","endLine":1,"endColumn":23},"no-native-reassign",["77"],"no-negated-in-lhs",["78"],"no-unused-vars","'Success' is defined but never used.","Identifier","unusedVar","'useHistory' is defined but never used.","'handleLogin' is assigned a value but never used.","'unsubscribe' is assigned a value but never used.","'useRef' is defined but never used.","no-global-assign","no-unsafe-negation"]