Input] = useState(\"\");\n\n  useEffect(() => {\n    db.collection(\"users\")\n      .orderBy(\"timestamp\", \"desc\")\n      .onSnapshot((snapshot) => {\n        setNeedies(\n          snapshot.docs.map((doc) => {\n            return { id: doc.id, needy: doc.data() };\n          })\n        );\n      });\n    console.log(needies);\n  }, []);\n\n  const handleChange = (e) => {\n    e.preventDefault();\n    var new_input = e.target.value;\n    setInput(new_input);\n\n    setDisplayArr(\n      needies.filter((firm) => {\n        return (\n          firm.needy.firm_name &&\n          firm.needy.firm_name\n            .toLowerCase()\n            .indexOf(new_input.toLowerCase()) !== -1\n        );\n      })\n    );\n  };\n\n  return (\n    <div className=\"needies\">\n      <div className=\"needies_headline\">\n        <h1>We need Funds!</h1>\n        <input\n          type=\"text\"\n          placeholder=\"Search here...\"\n          value={input}\n          onChange={handleChange}\n        />\n\n        {displayArr.length === 0\n          ? needies.map(({ id, needy }) => {\n              if (needy.firm_name) {\n                return (\n                  <Needy\n                    key={id}\n                    firm_id={id}\n                    amount={needy.amount}\n                    need_funds={needy.need_funds}\n                    firm_name={needy.firm_name}\n                    firm_image={needy.firm_image}\n                  />\n                );\n              }\n            })\n          : displayArr.map(({ id, needy }) => {\n              if (needy.firm_name) {\n                return (\n                  <Needy\n                    key={id}\n                    firm_id={id}\n                    amount={needy.amount}\n                    need_funds={needy.need_funds}\n                    firm_name={needy.firm_name}\n                    firm_image={needy.firm_image}\n                  />\n                );\n              }\n            })}\n      </div>\n 